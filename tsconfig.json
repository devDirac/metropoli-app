{
  "extends": "@react-native/typescript-config/tsconfig.json",


  "compilerOptions": {
    "baseUrl": "." ,
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@store/*": [
        "src/assets/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@views/*": [
        "src/views/*"
      ],
      "@theme/*": [
        "src/theme/*"
      ],
      "@config": [
        "src/config/*"
      ]
    },
  }
}
